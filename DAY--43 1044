#include <iostream>
using namespace std;

int main() {
    int T; 
    cin >> T;
    
    while (T--) {
        long long N;
        cin >> N;
        
        
        long long count_odd = (N + 1) / 2;
        long long count_even = N / 2;
       
        long long result = count_odd * count_even;
        
        cout << result << endl;
    }
    
    return 0;
}
