#include <iostream>
#include <vector>
using namespace std;

int main() {
    int T;
    cin >> T;
    
    while (T--) {
        int N;
        cin >> N;
        vector<int> B(N);
        int sum = 0;
        
        
        for (int i = 0; i < N; ++i) {
            cin >> B[i];
            sum += B[i];
        }
        
      
        if (sum % 2 == 0) {
            cout << "YES" << endl;
        } else {
            cout << "NO" << endl;
        }
    }
    
    return 0;
}
