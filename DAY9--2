#include <bits/stdc++.h>
using namespace std;
int main() {
    int x;
    cin >> x; 
    
    while (x--) {
        int n;
        cin >> n;  
        
        
        if (n % 2 == 0) {
            cout << "ALICE" << endl;
        } else {
            cout << "BOB" << endl;
        }
    }
    
    return 0;
}
