#include <iostream>
using namespace std;

int main() {
    int x;
    cin >> x;  
    while (x--) {
        int N;
        cin >> N;  
        int oddCount = 0;  
        for (int i = 0; i < N; ++i) {
            int num;
            cin >> num;
            if (num % 2 != 0) {  
                ++oddCount;
            }
        }

       
        if (oddCount >= 2) {
            cout << "YES\n";
        } else {
            cout << "NO\n";
        }
    }

    return 0;
}
