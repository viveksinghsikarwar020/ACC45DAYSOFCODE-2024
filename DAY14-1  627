#include <iostream>
#include <algorithm> /
using namespace std;

int main() {
    int X; 
    cin >> X;
    
    while (X--) {
        int a, b, c, d;
       
        cin >> a >> b >> c >> d;
      
        int max_tastiness = max({a + c, a + d, b + c, b + d});
        
        
        cout << max_tastiness << endl;
    }
    
    return 0;
}
